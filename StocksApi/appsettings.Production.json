{
    "AllowedHosts": "xxxxxxxxxxxx",
    "ConnectionStrings": {
        "STOCKSAPI_STOCKSDBCONNECTIONSTRING": "Filename=Stocks.db"
    },
    "Serilog": {
        // Log levels include:
        //     Verbose, Debug, Information, Warning, Error, Fatal
        "MinimumLevel": {
            "Default": "Error",
            "Override": {
                "Microsoft": "Error",
                "Microsoft.EntityFrameworkCore": "Error"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Enrich": [ "FromLogContext", "WithExceptionDetails" ]
    }
}